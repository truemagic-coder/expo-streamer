{
  "name": "expo-streamer",
  "version": "1.0.0",
  "description": "Enterprise-grade audio streaming and recording for Expo/React Native with zero-crash reliability, SOLID architecture, and comprehensive testing",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "source": "src/index.ts",
  "scripts": {
    "build": "expo-module build",
    "clean": "expo-module clean",
    "lint": "expo-module lint",
    "test": "jest",
    "test:ios": "xcodebuild test -workspace example/ios/expoaudiostreamexample.xcworkspace -scheme expoaudiostreamexample -destination 'platform=iOS Simulator,name=iPhone 14'",
    "test:android": "cd android && ./gradlew test",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:all": "npm run test && npm run test:ios && npm run test:android",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "prepare": "expo-module prepare",
    "postinstall": "husky",
    "prepublishOnly": "expo-module prepublishOnly",
    "expo-module": "expo-module",
    "open:ios": "open -a \"Xcode\" example/ios",
    "open:android": "open -a \"Android Studio\" example/android"
  },
  "keywords": [
    "react-native",
    "expo",
    "audio-streaming",
    "audio-recording",
    "real-time-audio",
    "typescript",
    "solid-principles",
    "zero-crash",
    "enterprise-grade",
    "voice-processing",
    "audio-playback"
  ],
  "repository": "https://github.com/truemagic-coder/expo-streamer",
  "bugs": {
    "url": "https://github.com/truemagic-coder/expo-streamer/issues"
  },
  "author": "Bevan Hunt <bevan@bevanhunt.com>",
  "contributors": [
    {
      "name": "Alexander Demchuk",
      "email": "alexander.demchuk@example.com",
      "url": "https://github.com/alexander-demchuk",
      "role": "Original Author"
    }
  ],
  "license": "MIT",
  "homepage": "https://github.com/truemagic-coder/expo-streamer#readme",
  "devDependencies": {
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.0.0",
    "@types/jest": "^29.0.0",
    "@types/react": "^18.0.25",
    "@types/react-test-renderer": "^19",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "eslint": "^9.35.0",
    "expo-module-scripts": "^3.4.1",
    "expo-modules-core": "^1.11.12",
    "husky": "^9.0.11",
    "jest": "^29.0.0",
    "react": "^19.1.1",
    "react-native": "^0.81.4",
    "react-test-renderer": "^19.1.1",
    "ts-jest": "^29.0.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "expo": "*",
    "react": "*",
    "react-native": "*"
  },
  "packageManager": "yarn@4.1.1"
}
